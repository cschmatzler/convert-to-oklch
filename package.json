{
  "name": "convert-to-oklch",
  "version": "1.0.3",
  "description": "CLI tool that converts rgb(), rgba(), hex and hsl() colors to oklch() in specified CSS files.",
  "main": "./src/index.js",
  "bin": {
    "convert-to-oklch": "./src/index.js"
  },
  "type": "commonjs",
  "files": [
    "src/**/*.js",
    "!**/**/*.test.js"
  ],
  "keywords": [
    "oklch",
    "convert",
    "transform",
    "rgb",
    "rgba",
    "hex",
    "hsl"
  ],
  "author": "Fedya Petrakov",
  "repository": "fpetrakov/convert-to-oklch",
  "license": "MIT",
  "scripts": {
    "test": "jest"
  },
  "dependencies": {
    "colorjs.io": "^0.4.0",
    "commander": "^9.4.0",
    "postcss": "^8.4.16"
  },
  "devDependencies": {
    "jest": "^28.1.3"
  }
}
